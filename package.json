{
  "name": "dentalink-code-test",
  "version": "1.0.0",
  "description": "This is a simple challenge where you can show all your skills. There are no rules, it's absolute open to however you want to solve it.",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "start": "node_modules/.bin/webpack-dev-server",
    "dev": "webpack -d --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/danielguajardok/dentalink-code-test.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/danielguajardok/dentalink-code-test#readme",
  "dependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "moment": "^2.18.1",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "webpack": "^3.6.0"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "jest": "^21.2.0",
    "react-test-renderer": "^16.0.0",
    "webpack-dev-server": "^2.8.2"
  },
  "jest": {
    "transform": {".*": "<rootDir>/node_modules/babel-jest"},
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      },
      "./js/components/**/*.js": {
        "branches": 40,
        "statements": 40
      },
      "./js/utils/*.js": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
